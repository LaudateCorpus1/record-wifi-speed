{
  "name": "record-wifi-speed",
  "version": "1.0.0-alpha.1",
  "description": "Record Wi-Fi speed",
  "author": "Samuel Fricis Ievins",
  "license": "MIT",
  "repository": "github:sievins/record-wifi-speed",
  "bin": "bin/record-wifi-speed.exe",
  "scripts": {
    "build": "pkg run.js --target host --output bin/record-wifi-speed.exe",
    "start": "node run.js"
  },
  "husky": {
    "hooks": {
      "pre-commit": "mv .env .temp && npm run build && mv .temp .env && git add bin"
    }
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "moment": "^2.24.0",
    "shelljs": "^0.8.3",
    "speedtest-net": "^1.5.1",
    "wifi-name": "^3.1.1"
  },
  "devDependencies": {
    "husky": "^2.5.0",
    "pkg": "^4.4.0"
  },
  "keywords": [
    "speed",
    "test",
    "record",
    "internet",
    "wifi",
    "download",
    "speed-test",
    "internet-speed",
    "wifi-speed",
    "download-speed"
  ]
}
